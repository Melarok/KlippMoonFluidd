[server]
host = 0.0.0.0
port = 7125

[job_queue]
load_on_startup = True

[authorization]
cors_domains = 
	http://*.local
	http://*.pi
trusted_clients = 
	10.0.0.0/8
	127.0.0.0/8
	172.0.0.0/8
	192.168.0.0/16

[octoprint_compat]

[history]

[update_manager]
enable_system_updates = False

[machine]
validate_service = False
validate_config = True
provider = none

[data_store]
temperature_store_size = 600
gcode_store_size = 1000

